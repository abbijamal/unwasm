{
  "name": "@fixture/wasm",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./index.wasm": {
      "types": "./index.wasm.d.ts",
      "default": "./dist/index.wasm"
    }
  },
  "scripts": {
    "asc": "pnpm --package=assemblyscript dlx asc",
    "build": "pnpm asc ./src/index.asc.ts -o dist/index.wasm -t dist/index.wat -b dist/index.js",
    "test": "NODE_OPTIONS='--experimental-wasm-modules' node -e 'import(`./dist/index.wasm`).then(mod => console.log(mod.rand(10, 20)))'"
  }
}
